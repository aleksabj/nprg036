<?xml version="1.0" encoding="UTF-8"?>
<festivalSchedule xmlns:xsi="http://www.w3.org/2001/XMLSchema"
                  xsi:noNamespaceSchemaLocation="../schemas/schema-1.xsd">

    <movies>
        <movie iri="http://example.org/movie/drive">
            <name xml:lang="en">Drive</name>
            <label xml:lang="en">Drive (2011)</label>
